{
  "expo": {
    "name": "MealsToGo",
    "slug": "MealsToGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F1F1F1"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maggy.mealstogo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#F1F1F1"
      },
      "package": "MealsToGo.V1",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBQseD3PE5z8o5EZpn3rBld7BUlYjmw8vM"
        }
      }
    },
    "web": {
      "favicon": "./assets/splash.png"
    },
    "plugins": [
      "expo-font",
      "expo-camera",
      {
        "cameraPermission": "Allow MealsToGo to access your camera",
        "microphonePermission": "Allow MealsToGo to access your microphone",
        "recordAudioAndroid": true
      }
    ],
    "extra": {
      "eas": {
        "projectId": "26ebe0a9-34bb-48f2-8712-3977d0833141"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/26ebe0a9-34bb-48f2-8712-3977d0833141"
    }
  }
}
